// SQLite + Prisma schema
// Run: npx prisma migrate dev --name init

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./database.db"
}

model Worker {
    id       String @id @default(cuid())
    name     String @unique
    email    String @unique
    position String
    salary   Float
}
